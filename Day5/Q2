class Solution{
    public int eraseOverlapIntervals(int[][] a){
        Arrays.sort(a,(x,y)->x[1]-y[1]);
        int end=a[0][1],count=1;
        for(int i=1;i<a.length;i++){
            if(a[i][0]>=end){
                count++;
                end=a[i][1];
            }
        }
        return a.length-count;
    }
}
